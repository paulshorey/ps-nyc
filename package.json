{
  "name": "ps",
  "description": "Personal portfolio site",
  "version": "0.1.0",
  "author": "Paul Shorey <hellol@paulshorey.com>",
  "bugs": {
    "url": "https://github.com/paulshorey/ps/issues"
  },
  "dependenciesNotUsed": {
    "express": "^4.17.1",
    "serve-handler": "^6.1.3"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/pro-light-svg-icons": "^5.15.1",
    "@fortawesome/pro-regular-svg-icons": "^5.15.1",
    "@fortawesome/pro-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "gatsby": "^2.27.4",
    "gatsby-plugin-react-helmet": "^3.5.0",
    "gatsby-plugin-sass": "^2.6.0",
    "gatsby-plugin-styled-components": "^3.5.0",
    "gatsby-plugin-styled-jsx": "^3.5.0",
    "horizontal_carousel": "^0.1.1",
    "luminous-lightbox": "^2.3.3",
    "node-sass": "^4.14.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "react-keydown": "^1.9.12",
    "src": "file:./src",
    "styled-components": "^5.0.1",
    "styled-jsx": "^3.3.0",
    "styled-jsx-plugin-sass": "^1.0.0",
    "typeface-quicksand": "^1.1.13"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "babel-preset-gatsby": "^0.10.0",
    "html-loader": "^1.3.0",
    "markdown-loader": "^6.0.0",
    "prettier": "^2.1.0",
    "sharp": "^0.26.3"
  },
  "homepage": "https://github.com/paulshorey/ps",
  "keywords": [
    "gatsby",
    "contentful"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paulshorey/ps.git"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "npm run ip_local && GATSBY_ACTIVE_ENV=development gatsby develop -p 1090",
    "build": "GATSBY_ACTIVE_ENV=production gatsby build",
    "pub": "rm -rf ./../public/ps && mv ./public ./../public/ps && cd ./../public && git add . && git commit -m 'ps' && git push",
    "thumb": "node image-thumbnails ./static/screenshots",
    "#######": "#",
    "# etc #": "#",
    "######!": "#",
    "ls": "ls ./../ps",
    "sami": "cp -R static/sami/* public/ps/sami && npm run pub",
    "files": "cp -R static/files/* ./../public/ps/files && npm run pub",
    "serve": "echo \"serving production @ port 9090\" && http-server build_stage -p 9090",
    "pull": "git add . && rm package-lock.json && git reset --hard HEAD && git pull",
    "ip_stage": "echo \"\nPUBLIC_API_HOST = '$(ipconfig getifaddr en0):1090'\nADMIN_API_HOST = '$(ipconfig getifaddr en0):1090'\" >> .env.development",
    "ip_local": "echo \"\nPUBLIC_API_HOST = '127.0.0.1:1090'\nADMIN_API_HOST = '127.0.0.1:1090'\" >> .env.development",
    "lint": "eslint --ext .js,.jsx --ignore-pattern public .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js' 'src/**/*.md' 'bin/*.js'",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "fix-semi": "eslint --quiet --ignore-pattern node_modules --ignore-pattern public --parser babel-eslint --no-eslintrc --rule '{\"semi\": [2, \"never\"], \"no-extra-semi\": [2]}' --fix *.js bin/*.js",
    "contentful-postinstall": "node ./bin/hello.js",
    "contentful-setup": "node ./bin/setup.js",
    "contentful-cleanup": "rimraf bin contentful && yarn remove contentful-import chalk inquirer rimraf "
  }
}
